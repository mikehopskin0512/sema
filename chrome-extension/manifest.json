{
  "name": "Sema for Chrome",
  "description": "Sema chrome extension for github",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*",
        "*://*.github.com/*"
      ],
      "js": [
        "/libs/cash.min.js",
        "main.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/sema16.png",
      "32": "/images/sema32.png",
      "48": "/images/sema48.png",
      "128": "/images/sema128.png"
    }
  },
  "icons": {
    "16": "/images/sema16.png",
    "32": "/images/sema32.png",
    "48": "/images/sema48.png",
    "128": "/images/sema128.png"
  },
  "options_page": "options.html"
}