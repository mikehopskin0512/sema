{
  "name": "apollo",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "^16"
  },
  "scripts": {
    "start": "nodemon --exec babel-node --inspect=0.0.0.0 src/app.js --max-http-header-size=16384",
    "build": "npm run clean && babel src --out-dir dist --copy-files",
    "serve": "node dist/app.js",
    "clean": "rm -rf ./dist && mkdir dist",
    "importCommentsAndGuides": "nodemon --exec babel-node scripts/importCommentsAndGuides.js",
    "fixSlugs": "nodemon --exec babel-node scripts/fixSlugs.js",
    "lint": "eslint ."
  },
  "dependencies": {
    "@octokit/auth": "^2.0.0",
    "@octokit/rest": "^18.0.0",
    "@sendgrid/mail": "^7.2.1",
    "aws-sdk": "^2.796.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.12",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.22.1",
    "date-fns-tz": "^1.1.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-async-handler": "^1.1.4",
    "flexsearch": "^0.6.32",
    "google-spreadsheet": "^3.1.15",
    "http-errors": "~1.6.3",
    "http-status": "^1.4.2",
    "json2csv": "^4.5.4",
    "jsonwebtoken": "^8.5.1",
    "lang": "^0.1.1",
    "lodash": "^4.17.15",
    "md5": "^2.3.0",
    "method-override": "^3.0.0",
    "migrate-mongo": "^8.2.3",
    "mongodb-extended-json": "^1.11.1",
    "mongoose": "^5.9.15",
    "multer": "1.4.1",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "^8.2.1",
    "request": "^2.88.2",
    "speakingurl": "^14.0.1",
    "swagger-ui-express": "^4.3.0",
    "text-clipper": "^2.1.0",
    "uuid": "^8.1.0",
    "xlsx": "^0.15.5",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "nodemon": "^2.0.4"
  }
}
